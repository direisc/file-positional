{
  "name": "file-positional",
  "private": false,
  "version": "1.2.2",
  "description": "Generate a flat positional text file of your dataset",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "test": "npx vitest run",
    "watch": "npx vitest",
    "lint": "eslint --cache src/*.ts",
    "lint:fix": "eslint --fix --cache src/*.ts",
    "typecheck": "npx tsc --noEmit",
    "prettier:check": "npx prettier -c src",
    "prettier:fix": "npx prettier -w src",
    "_build": "rimraf dist && npx tsc",
    "build": "rimraf dist && tsup src/index.ts --format cjs,esm --sourcemap --dts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "positional",
    "file-generator",
    "flat-file",
    "positional-dataset"
  ],
  "author": "direisc",
  "license": "MIT",
  "homepage": "https://github.com/direisc/file-positional#readme",
  "repository": "github:direisc/file-positional",
  "bugs": {
    "url": "https://github.com/direisc/file-positional/issues"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.2.5",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
